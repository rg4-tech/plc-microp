PROGRAM
RUNG 1
  COMMENT RG 4 Tech - Tutorial Youtube LDmicro - https://www.youtube.com/@rg4tech\r\nImpianto antifurto casa
END
RUNG 2
  COMMENT Selettore a chiave e Linea di guardia (tamper)\r\n
END
RUNG 3
  CONTACTS Xtamper 1 1
  OSR
  CONTACTS Xsel_chiave 1 1
  PARALLEL
    COIL Rallarme_ON 0 1 0 0
    COIL Rmem_tamper 0 1 0 0
  END
END
RUNG 4
  CONTACTS Xsel_chiave 1 1
  OSF
  PARALLEL
    COIL Rallarme_ON 0 0 1 0
    COIL Rmem_tamper 0 0 1 0
    COIL Rmem_finestre 0 0 1 0
    COIL Rmem_rilev 0 0 1 0
    COIL Rmem_porta_ingr 0 0 1 0
  END
END
RUNG 5
  COMMENT Inseritore
END
RUNG 6
  CONTACTS Xsel_chiave 1 1
  CONTACTS Xinseritore 1 1
  COIL Rinserito 0 0 0 0
END
RUNG 7
  COMMENT Reset memorie all'accensione impianto
END
RUNG 8
  CONTACTS Rinserito 0 0
  OSR
  PARALLEL
    COIL Rmem_tamper 0 0 1 0
    COIL Rmem_finestre 0 0 1 0
    COIL Rmem_rilev 0 0 1 0
    COIL Rmem_porta_ingr 0 0 1 0
  END
END
RUNG 9
  COMMENT Spegnimento allarme_ON da inseritore
END
RUNG 10
  CONTACTS Xinseritore 0 1
  CONTACTS Rmem_tamper 1 0
  COIL Rallarme_ON 0 0 1 0
END
RUNG 11
  COMMENT Serie finestre
END
RUNG 12
  CONTACTS Xserie_finestre 1 1
  OSR
  CONTACTS Rinserito 0 0
  PARALLEL
    COIL Rallarme_ON 0 1 0 0
    COIL Rmem_finestre 0 1 0 0
  END
END
RUNG 13
  COMMENT Pulsante esclusione rilevatori
END
RUNG 14
  CONTACTS Xexcl_rilevat 1 1
  CONTACTS Rinserito 1 0
  COIL Rrilev_esclusi 0 0 0 1
END
RUNG 15
  COMMENT Serie rilevatori
END
RUNG 16
  CONTACTS Xserie_rilev 1 1
  OSR
  CONTACTS Rinserito 0 0
  CONTACTS Rrilev_esclusi 1 0
  PARALLEL
    COIL Rallarme_ON 0 1 0 0
    COIL Rmem_rilev 0 1 0 0
  END
END
RUNG 17
  COMMENT Porta ingresso
END
RUNG 18
  CONTACTS Xporta_ingr 1 1
  OSR
  CONTACTS Rinserito 0 0
  PARALLEL
    COIL Rallarme_ON 0 1 0 0
    COIL Rmem_porta_ingr 0 1 0 0
  END
END
RUNG 19
  COMMENT Sirene
END
RUNG 20
  PARALLEL
    CONTACTS Rinserito 0 0
    CONTACTS Rmem_tamper 0 0
  END
  CONTACTS Rallarme_ON 0 0
  TON T1 8000000 0
  COIL RT1 0 0 0 0
END
RUNG 21
  PARALLEL
    CONTACTS Rinserito 0 0
    CONTACTS Rmem_tamper 0 0
  END
  CONTACTS Rallarme_ON 0 0
  CONTACTS RT1 1 0
  PARALLEL
    COIL Ysir_interna 0 0 0 0
    COIL Ysir_esterna 1 0 0 0
    SERIES
      OSF
      COIL Rallarme_ON 0 0 1 0
    END
  END
END
RUNG 22
  COMMENT Allarme generale
END
RUNG 23
  PARALLEL
    CONTACTS Rmem_tamper 0 0
    CONTACTS Rmem_finestre 0 0
    CONTACTS Rmem_rilev 0 0
    CONTACTS Rmem_porta_ingr 0 0
  END
  COIL Rallarme_Gen 0 0 0 0
END
RUNG 24
  COMMENT Clock
END
RUNG 25
  TCY Tclock 400000 0
  COIL Rclock 0 0 0 0
END
RUNG 26
  CONTACTS Raux2_clock 1 0
  TON T2 1000000 0
  COIL Rclock_breve 0 0 0 0
END
RUNG 27
  CONTACTS Rclock_breve 0 0
  TON T3 100000 0
  COIL Raux2_clock 0 0 0 0
END
RUNG 28
  COMMENT Led segnalazione
END
RUNG 29
  CONTACTS Rmem_tamper 0 0
  CONTACTS Rclock 0 0
  COIL Yled_tamper 0 0 0 0
END
RUNG 30
  CONTACTS Rmem_finestre 0 0
  CONTACTS Rclock 0 0
  COIL YLed_finestre 0 0 0 0
END
RUNG 31
  CONTACTS Rmem_rilev 0 0
  CONTACTS Rclock 0 0
  COIL Yled_rilev 0 0 0 0
END
RUNG 32
  CONTACTS Rmem_porta_ingr 0 0
  CONTACTS Rclock 0 0
  COIL Yled_porta_ing 0 0 0 0
END
RUNG 33
