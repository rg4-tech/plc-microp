  CONTACTS Xporta_ingr 0 1
  PARALLEL
    CONTACTS Xserie_rilev 0 1
    CONTACTS Rrilev_esclusi 0 0
  END
  CONTACTS Xtamper 0 1
  COIL Yled_linee_OK 0 0 0 0
END
RUNG 34
  CONTACTS Rinserito 0 0
  COIL RL_inserito 0 0 0 0
END
RUNG 35
  CONTACTS Rallarme_Gen 0 0
  CONTACTS Rclock 0 0
  COIL RL_inserito1 0 0 0 0
END
RUNG 36
